        <mesh name="finger_col" file="../franka_sim/meshes/collision/finger.stl" scale='1.75 1.0 1.75'/>
        <mesh name="finger_viz" file="../franka_sim/meshes/collision/finger.stl" scale='1.75 1.0 1.75'/>
